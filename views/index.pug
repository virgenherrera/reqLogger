extends layout

block content
  div.row
    div.col-md-12
        h1= title
  
  div.row
    div.col-md-12.well#content
      each row,k in rows
        div.thumbnail
          div.caption
            h3= 'Request #' + ( parseInt(k) + 1 )
            p= "Created At: " + row.createdAt
            h4 received data
            pre= JSON.stringify( row.body , null, 4 )
               
      
      
  div.row
    div.col-md-2
      p#count= "total rows: "+count
